{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@src/*": [
        "src/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@article/*": [
        "src/components/article/*"
      ],
      "@ui/*": [
        "src/components/ui/*"
      ],
    },
    "strictNullChecks": true,
  },
}